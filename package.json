{
  "name": "gitme",
  "description": "A command line tool to see your git commits across multiple projects",
  "version": "0.0.3",
  "author": "Dom Harrington <dom.harrington@clock.co.uk>",
  "contributors": [
    "Dom Harrington <dom.harrington@clock.co.uk>"
  ],
  "dependencies": {
    "async": ">= 0.1.9 < 0.2.0",
    "colors": "0.6.0-1",
    "underscore": "1.3.3",
    "commander": "0.6.0"
  },
  "devDependencies": {
    "mocha": "0.3.6",
    "should": "0.5.0"
  },
  "keywords": [
    "git",
    "log",
    "commit"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/domharrington/gitme.git"
  },
  "main": "index",
  "scripts": {
    "test": "mocha -r should -R List",
    "prepublish": "npm prune"
  },
  "bin": {
    "gitme": "./bin/gitme"
  },
  "engines": {
    "node": ">= 0.4.6 < 0.7.0"
  },
  "preferGlobal": true
}
